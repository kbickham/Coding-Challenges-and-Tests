def checkio(data: str) -> bool:
 secondary_criteria = [False,False,False] #numerix,upperC,lowerC
 if len(data) >= 10:
     for chars in data:
        if (ord(chars)>=48 and ord(chars) <= 57):
            secondary_criteria[0]=True;
        if (ord(chars)>=65 and ord(chars) <= 90):
            secondary_criteria[1]=True;
        if (ord(chars)>=97 and ord(chars) <= 122):
            secondary_criteria[2]=True;
     
    #replace this for solution
 if secondary_criteria == [True,True,True]:
      return True
 else:
      return False
